{
  "dart.lineLength": 120,
  "[dart]": {
    "editor.rulers": [
        120
    ],
  },
  "dart.flutterSdkPath": ".fvm/flutter_sdk",
  // Remove .fvm files from search
  "search.exclude": {
    "**/.fvm": true
  },
  // Remove from file watching
  "files.watcherExclude": {
    "**/.fvm": true
  },
  "cmake.sourceDirectory": "${workspaceFolder}/linux"
}