{
  "dart.lineLength": 120,
  "[dart]": {
    "editor.rulers": [
        120
    ],
  },
  "[arb]": {
    "editor.tabSize": 2,
  },
  "dart.flutterSdkPath": ".fvm/flutter_sdk",
  // Remove .fvm files from search
  "search.exclude": {
    "**/.fvm": true
  },
  // Remove from file watching
  "files.watcherExclude": {
    "**/.fvm": true
  },
  "java.configuration.updateBuildConfiguration": "automatic"
}